{
  "expo": {
    "name": "sport-connect-app",
    "slug": "sport-connect-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "your-app-scheme",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.interbios.sportconnectapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      ["expo-router"]
      // [
      //   "expo-font",
      //   {
      //     "fonts": [
      //       "node_modules/@expo-google-fonts/noto-sans-thai/NotoSansThai_400Regular.ttf",
      //       "node_modules/@expo-google-fonts/noto-sans-thai/NotoSansThai_700Bold.ttf",
      //       "node_modules/@expo-google-fonts/noto-sans-thai/NotoSansThai_900Black.ttf"
      //     ]
      //   }
      // ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8881da1b-a4d3-49b6-9e71-83394553dfcd"
      }
    },
    "owner": "interbios"
  }
}
