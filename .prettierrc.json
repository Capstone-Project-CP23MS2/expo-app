{
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "bracketSameLine": true,
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false,
    "array-bracket-spacing": [
        "error",
        "always"
    ],
    "function-call-argument-newline": [
        "error",
        "always"
    ]
}